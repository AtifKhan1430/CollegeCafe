<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".vendorHome"
    android:background="@drawable/background">

    <ImageButton
        android:id="@+id/food"
        style="?attr/imageButtonStyle"
        android:layout_width="160dp"
        android:layout_height="168dp"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="33dp"
        android:layout_marginTop="550dp"
        android:layout_marginEnd="200dp"
        android:layout_marginRight="200dp"
        android:layout_marginBottom="474dp"
        android:background="@drawable/rounded_corners_imagebutton"
        app:srcCompat="@drawable/food" />

    <ImageButton
        android:id="@+id/hotel"
        style="?attr/imageButtonStyle"
        android:layout_width="160dp"
        android:layout_height="168dp"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="215dp"
        android:layout_marginTop="550dp"
        android:layout_marginEnd="18dp"
        android:layout_marginRight="18dp"
        android:layout_marginBottom="475dp"
        android:background="@drawable/rounded_corners_imagebutton"
        app:srcCompat="@drawable/hotel" />

    <ImageButton
        android:id="@+id/stationary"
        style="?attr/imageButtonStyle"
        android:layout_width="168dp"
        android:layout_height="168dp"
        android:layout_alignParentStart="false"
        android:layout_alignParentLeft="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="117dp"
        android:layout_marginTop="550dp"
        android:layout_marginEnd="108dp"
        android:layout_marginRight="108dp"
        android:layout_marginBottom="258dp"
        android:background="@drawable/rounded_corners_imagebutton"
        app:srcCompat="@drawable/stationary" />

    <Button
        android:id="@+id/order"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="148dp"
        android:layout_marginTop="585dp"
        android:fontFamily="@font/alegreya_sans_sc_extrabold"
        android:text="VIEW ORDERS"
        android:textColor="@android:color/background_dark"
        android:textColorHint="@android:color/darker_gray" />

    <TextView
        android:id="@+id/textView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="72dp"
        android:fontFamily="@font/alegreya_sans_sc_extrabold"
        android:text="WELCOME VENDOR"
        android:textColor="@android:color/background_dark"
        android:textSize="24sp" />

    <TextView
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="11dp"
        android:text="TextView" />

</RelativeLayout>